import java.awt.Image;

import javax.swing.*;

public class Breakout extends JFrame{
	
	 
	
	
	public Breakout() {
		
		
		super("Breakout");
		setSize(700, 600);
		setLocation(400, 200);

		
		
		
		Game game = new Game();
		
		

		add(game);
		setDefaultCloseOperation(WindowConstants.EXIT_ON_CLOSE);
		setVisible(true);
		setResizable(false);
		
	}

}
